<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cib.cap4j.cfn.statistics.history.dao.IIpExecutetimeDao">
	<resultMap id="ipExecutetimeMap" type="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO">
		<id column="ZJ" property="zj" />
		<result column="JLSJ" property="jlsj" />
		<result column="GSRQ" property="gsrq" />
		<result column="ZXNR" property="zxnr" />
		<result column="ZXSC" property="zxsc" />
		<result column="ZXCS" property="zxcs" />
		<result column="JLLX" property="jllx" />
		<result column="GSJD" property="gsjd" />
	</resultMap>

	<select id="findAllInfosCount" resultType="int">
		   select count(*)
		   from ip_executetime 
		   where 1=1
		<if test="gsrq != null and gsrq != ''"> and GSRQ = #{gsrq}</if>
		<if test="jllx != null and jllx != ''"> and JLLX = #{jllx}</if>
	</select>

	<select id="findAllInfos" parameterType="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO"
		resultMap="ipExecutetimeMap">
		select ZJ,JLSJ,GSRQ,ZXNR,ZXSC ZXSC,ZXCS,JLLX,GSJD
		from ip_executetime 
		where 1=1
		<if test="gsrq != null and gsrq != ''"> and GSRQ = #{gsrq}</if>
		<if test="jllx != null and jllx != ''"> and JLLX = #{jllx}</if>
        order by GSRQ desc,ZXSC desc
	</select>

	<insert id="save" parameterType="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO">
		INSERT INTO ip_executetime
		(ZJ,JLSJ,GSRQ,ZXNR,ZXSC,ZXCS,JLLX,GSJD)
		values
		(#{zj,jdbcType=CHAR},#{jlsj,jdbcType=TIMESTAMP},#{gsrq,jdbcType=DATE},#{zxnr,jdbcType=VARCHAR},#{zxsc,jdbcType=FLOAT},#{zxcs,jdbcType=NUMERIC},#{jllx,jdbcType=CHAR},#{gsjd,jdbcType=VARCHAR})
	</insert>

	<update id="update" parameterType="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO">
		update ip_executetime
		<trim prefix="SET" suffixOverrides=",">
			<if test="zj != null"> ZJ = #{zj},</if>
			<if test="jlsj != null"> JLSJ = #{jlsj},</if>
			<if test="gsrq != null"> GSRQ = #{gsrq},</if>
			<if test="zxnr != null"> ZXNR = #{zxnr},</if>
			<if test="zxsc != null"> ZXSC = #{zxsc},</if>
			<if test="zxcs != null"> ZXCS = #{zxcs},</if>
			<if test="jllx != null"> JLLX = #{jllx},</if>
			<if test="gsjd != null"> GSJD = #{gsjd},</if>
		</trim>
		where
		<trim prefix="" suffixOverrides="AND">
			<if test="zj != null">ZJ = #{zj} AND </if>
		</trim>
	</update>

	<delete id="delete" parameterType="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO">
		delete from ip_executetime where ZJ in
		<foreach collection="array" item="oneId" open="(" close=")"
			separator=",">
			#{oneId}
		</foreach>
	</delete>

    <delete id="deleteByDate" parameterType="com.cib.cap4j.cfn.statistics.history.vo.IpExecutetimeVO">
		delete from ip_executetime 
		where GSRQ <![CDATA[<]]> #{gsrq}
	</delete>
	
	<select id="getIpExecutetimeVOById" parameterType="string"
		resultMap="ipExecutetimeMap">
		select ZJ,JLSJ,GSRQ,ZXNR,ZXSC,ZXCS,JLLX,GSJD
		from ip_executetime where ZJ = #{value}
	</select>
</mapper>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
   * 用户历史密码mybatis实体映射
   *
   * Created on 2014-5-16
   * @author 张玉清 
   * @reviewer 
-->
<mapper namespace="com.cib.cap4j.cfn.common.arcuserauth.dao.IIPArcPwdHistDao">
	<resultMap id="arcpwdhistVO_result" type="com.cib.cap4j.cfn.common.arcuserauth.vo.IPArcPwdHistVO">
        <id column="ZJ" property="zj" />
        <result column="YHBH" property="yhbh" />
        <result column="LSMM" property="lsmm" />
        <result column="ZHXGR" property="zhxgr" />
        <result column="ZHXGSJ" property="zhxgsj" />
    </resultMap>
	
    <select id="findAllByYhbh" resultMap="arcpwdhistVO_result" parameterType="java.lang.String">
        select * from IP_ARCPWDHIST where YHBH = #{yhbh} order by ZHXGSJ DESC
    </select>
    
    <insert id="save" parameterType="com.cib.cap4j.cfn.common.arcuserauth.vo.IPArcPwdHistVO">
        INSERT INTO IP_ARCPWDHIST 
        (ZJ, YHBH, LSMM, ZHXGR, ZHXGSJ)
		values
		(#{zj,jdbcType=VARCHAR}, #{yhbh,jdbcType=VARCHAR}, #{lsmm,jdbcType=VARCHAR},  #{zhxgr,jdbcType=VARCHAR}, #{zhxgsj,jdbcType=TIMESTAMP})
    </insert>
        
    <delete id="deletePwdHistByYhbh" parameterType="com.cib.cap4j.cfn.common.arcuserauth.vo.IPArcPwdHistVO">
        DELETE FROM IP_ARCPWDHIST 
         WHERE YHBH = #{yhbh}
    </delete>
    
</mapper>